{
  "name": "exwife",
  "version": "0.0.1",
  "description": "my ex-wife in the news",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node --enable-source-maps dist/main.js",
    "dev": "NODE_ENV=development ts-node -T src/main.ts -- local",
    "dev-list": "NODE_ENV=development ts-node -T src/main.ts -- list",
    "build": "rm -rf dist/ && tsc",
    "test": "run-p -cl test:*",
    "test:ts": "tsc --noEmit",
    "test:eslint": "eslint --color 'src/**/*' --max-warnings=0",
    "test:vitest": "vitest run --color",
    "test:prettier": "prettier 'src/**/*' -l",
    "prettier": "prettier 'src/**/*' --write",
    "vitest": "vitest --color"
  },
  "author": "mm <https://github.com/mamaruxo>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@cliqz/adblocker-puppeteer": "^1.23.2",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^8.40.0",
    "feedparser": "^2.2.10",
    "puppeteer": "^13.0.1",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "twoot": "0.4.0",
    "znv": "^0.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/feedparser": "^2.2.5",
    "@types/node": "^20.17.7",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-lostfictions": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.5"
  },
  "packageManager": "pnpm@9.7.1+sha512.faf344af2d6ca65c4c5c8c2224ea77a81a5e8859cbc4e06b1511ddce2f0151512431dd19e6aff31f2c6a8f5f2aced9bd2273e1fed7dd4de1868984059d2c4247"
}
