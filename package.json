{
  "name": "exwife",
  "version": "0.0.1",
  "description": "my ex-wife in the news",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "NODE_ENV=development ts-node -T src/main.ts -- local",
    "dev-list": "NODE_ENV=development ts-node -T src/main.ts -- list",
    "build": "rm -rf dist/ && tsc",
    "test": "run-s -l test:*",
    "test:ts": "tsc --noEmit",
    "test:eslint": "eslint --color 'src/**/*'",
    "test:jest": "jest --colors",
    "test:prettier": "prettier 'src/**/*' -l",
    "prettier": "prettier 'src/**/*' --write",
    "jest": "jest --colors --watch",
    "postinstall": "patch-package"
  },
  "author": "mm <https://github.com/mamaruxo>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@cliqz/adblocker-puppeteer": "^1.22.7",
    "@sentry/integrations": "^6.13.2",
    "@sentry/node": "^6.13.2",
    "async-retry": "^1.3.3",
    "envalid": "^6.0.2",
    "feedparser": "^2.2.10",
    "masto": "^4.1.16",
    "node-fetch": "^2.6.5",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "puppeteer": "^10.4.0",
    "puppeteer-extra": "^3.2.1",
    "puppeteer-extra-plugin-stealth": "^2.8.1",
    "source-map-support": "^0.5.20",
    "twitter-api-client": "^1.3.8",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.3",
    "@types/feedparser": "^2.2.4",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/node-fetch": "^2.5.12",
    "@types/uuid": "^8.3.1",
    "eslint": "^7.32.0",
    "eslint-config-lostfictions": "^1.4.1",
    "jest": "^27.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=14"
  },
  "jest": {
    "preset": "ts-jest",
    "rootDir": "src",
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    }
  }
}
